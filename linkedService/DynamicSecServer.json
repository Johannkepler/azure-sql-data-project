{
	"name": "DynamicSecServer",
	"properties": {
		"parameters": {
			"SecCikNumber": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat('https://www.sec.gov/Archives/edgar/data/', linkedService().SecCikNumber, '/index.json')}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}